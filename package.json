{
  "name": "node-simple-ws",
  "version": "1.0.0",
  "description": "Simple Wrapper over ws to use Websockets to easily with ng-websockets-apps",
  "license": "MIT",
  "repository": "tyderion/node-simple-ws",
  "author": {
    "name": "Gabriel Nadler",
    "email": "nadler.gabriel@gmail.com",
    "url": "github.com/tyderion"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo --space=4 && mocha"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    ""
  ],
  "dependencies": {
    "ws": "^0.8.0"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "chai-spies": "^0.7.1",
    "mocha": "^2.2.5",
    "xo": "^0.9.0"
  },
  "xo": {
    "envs": [
      "node",
      "mocha"
    ],
    "ignores": [
      "out/**"
    ],
    "rules": {
      "space-before-function-paren": [
        2,
        {
          "anonymous": "never",
          "named": "never"
        }
      ]
    }
  }
}
